{
  "manifest_version": 3,
  "name": "Image Downloader",
  "version": "1.0",
  "description": "Select and download images from a web page",
  "permissions": [
    "downloads",
    "activeTab",
    "scripting"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html"
  }
}
